<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="DarkBackGroundColor">#FF262626</Color>
    <SolidColorBrush Color="{StaticResource DarkBackGroundColor}" x:Key="DarkBackGroundBrush"/>
    <SolidColorBrush Color="#3D3D40" x:Key="DarkMenuBackGroundBrush"/>
    <SolidColorBrush Color="#AAAAAA" x:Key="TextForegroundBrush"/>
    <SolidColorBrush Color="#252526" x:Key="GridBackgroundBrush"/>
    <SolidColorBrush Color="#ABADB3" x:Key="LightBorderBrush"/>
    <SolidColorBrush Color="#1C1C1C" x:Key="MainAreaBackgroundBrush"/>
    <SolidColorBrush Color="#221E90FF" x:Key="SelectedBackBrush"/>
</ResourceDictionary>