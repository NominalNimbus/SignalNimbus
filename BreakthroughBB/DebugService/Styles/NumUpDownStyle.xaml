    <SolidColorBrush x:Key="TextBox_DisabledBorder"
                     x:Uid="SolidColorBrush_12"
                     Color="#131313" />
    <SolidColorBrush x:Key="TextBox_DisabledBackground"
                     x:Uid="SolidColorBrush_13"
