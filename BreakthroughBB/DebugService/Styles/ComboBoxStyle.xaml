                                              HorizontalContentAlignment="Right"
                                              Background="{StaticResource GlossyBlack_BackgroundGradient}"
                                              BorderBrush="{StaticResource GlossyBlack_StrokeGradient}"
                                              BorderThickness="{TemplateBinding BorderThickness}"
                                              Foreground="{StaticResource GlossyBlack_GreyStateBackground}"
                                              IsChecked="{Binding IsDropDownOpen,
                                                                  Mode=TwoWay,
                                                                  RelativeSource={RelativeSource TemplatedParent}}"
                                              x:Uid="DropDownToggle">
